n1 = int(input('Enter the 1st number: '))
n2 = int(input('Enter the 2nd number: '))
n3 = int(input('Enter the 3rd number: '))

# Sorting logic
if n1 <= n2 and n1 <= n3:
    if n2 <= n3:
        print(n1, n2, n3)
    else:
        print(n1, n3, n2)
elif n2 <= n1 and n2 <= n3:
    if n1 <= n3:
        print(n2, n1, n3)
    else:
        print(n2, n3, n1)
else:
    if n1 <= n2:
        print(n3, n1, n2)
    else:
        print(n3, n2, n1)
